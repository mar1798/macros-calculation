{
  "name": "drewl",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "npm run build -w $WORKSPACE",
    "start": "npm start -w $WORKSPACE",
    "clear": "turbo run clear",
    "test": "turbo run test",
    "barrels": "turbo run barrels",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "check": "turbo run check",
    "format": "prettier --write \"./**/*.{js,ts,tsx,jsx,json,md}\""
  },
  "devDependencies": {
    "prettier": "^3.3.3",
    "turbo": "latest"
  },
  "packageManager": "npm@10.8.3",
  "engines": {
    "node": "20.11.0",
    "npm": "10.8.3"
  }
}
